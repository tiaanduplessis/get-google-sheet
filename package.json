  },
  "repository": {
    "url": "tiaanduplessis/get-google-sheet",
    "type": "git"
  },
  "author": "tiaanduplessis",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  }
